<?xml version="1.0" encoding="UTF-8"?>
<!-- /project1/src/mapper/PointBoardMapper.xml -->
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.PointBoardMapper">
	<select id="select" parameterType="map" resultType="PointBoard">
		select * from pointboard 
		<if test="no != null">
			where no = #{no}
		</if>
		order by point
	</select>
</mapper>
